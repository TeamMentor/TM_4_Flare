mixin copyright
    footer
        a(href='http://www.securityinnovation.com/' target='_blank')
            .si-logo
        .icons
            ul
                li
                    .label.no-underline TEAM Mentor 4.0
                li
                    a(href='mailto:support@securityinnovation.com' title='Email')
                        +mail-icon
                li
                    a(href='http://www.twitter.com/SecInnovation' target='_blank' title='Twitter')
                        +twitter-icon
                li
                    a#terms-and-conditions(href='../misc/terms-and-conditions' title ='Terms and Conditions')
                        +paperclip-icon

