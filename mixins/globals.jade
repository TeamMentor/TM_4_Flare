//- Buttons
mixin open-button
    button.controls.dropdown(href='#', data-dropdown='article-open', aria-controls='drop1', aria-expanded='false')
        i.fi-plus
    ul#article-open.f-dropdown.active(data-dropdown-content='', aria-hidden='true', tabindex='-1')
        li
            a(href='#') ASP.NET
        li
            a(href='#') Android
        li
            a(href='#') C++
        li
            a(href='#') HTML5
        li
            a(href='#') iOS
        li
            a(href='#') Java
        li
            a(href='#') PHP
        li
            a(href='#') Scala

mixin save-button
    button.btn-result.icon-Save(title='Save article to Collection')

mixin delete-button
    button.btn-result.icon-Delete(title='Delete article from Collection')

mixin share-button
    button.btn-result.icon-Share(title='Share article')

mixin save-results-button
    button.btn-result.icon-Save(title='Save results to Collection')

mixin delete-results-button
    button.btn-result.icon-Delete(title='Delete results from Collection')

mixin share-results-button
    button.btn-result.icon-Share(title='Share results')

mixin clear-selected-button
